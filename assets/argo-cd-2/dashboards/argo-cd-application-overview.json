            "defaults": {
               "links": [
                  {
                     "targetBlank": true,
                     "title": "Go To Application",
                     "url": "https://github.com/adinhodovic/argo-cd-mixin?tab=readme-ov-file#argocd-badges"
                  }
               ],
               "thresholds": {
                  "steps": [ ]
               },
               "unit": "short"
            },
            "overrides": [
               {
                  "matcher": {
                     "id": "byName",
                     "options": "health_status"
                  },
                  "properties": [
                     {
                        "id": "color",
                        "value": {
                           "fixedColor": "yellow",
                           "mode": "fixed"
                        }
                     },
