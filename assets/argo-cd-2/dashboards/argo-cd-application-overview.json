{
   "__inputs": [ ],
   "__requires": [ ],
   "annotations": {
      "list": [ ]
   },
   "description": "A dashboard that monitors ArgoCD with a focus on the applications of ArgoCD. The dashboards were generated using [argo-cd-mixin](https://github.com/adinhodovic/argo-cd-mixin). Open issues and create feature requests in the repository.",
   "editable": false,
   "links": [
      {
         "asDropdown": true,
         "includeVars": false,
         "keepTime": true,
         "tags": [
            "ci/cd",
            "argo-cd"
         ],
         "targetBlank": true,
         "title": "ArgoCD",
         "type": "dashboards"
      }
   ],
   "panels": [
      {
         "collapsed": false,
         "gridPos": {
            "h": 1,
            "w": 24,
            "x": 0,
            "y": 0
         },
         "id": 1,
         "title": "Summary By Project",
         "type": "row"
      },
      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "A timeseries panel showing the health status of applications managed by ArgoCD.",
         "fieldConfig": {
            "defaults": {
               "custom": {
                  "axisSoftMin": 0,
                  "fillOpacity": 100,
                  "lineWidth": 1,
                  "stacking": {
                     "mode": "normal"
                  }
               },
               "decimals": 0,
               "unit": "short"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 6,
            "w": 12,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "options": {
            "legend": {
               "calcs": [
                  "mean",
                  "max"
               ],
               "displayMode": "table",
               "placement": "right",
               "showLegend": true,
               "sortBy": "Mean",
               "sortDesc": true
            },
            "tooltip": {
               "mode": "multi",
               "sort": "desc"
            }
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "exemplar": false,
               "expr": "sum(\n  argocd_app_info{\n    cluster=\"$cluster\",\nnamespace=~\"$namespace\",\njob=~\"$job\"\n\n,\ndest_server=~\"$kubernetes_cluster\",\nproject=~\"$project\"\n\n  }\n) by (job, project, health_status)\n",
               "legendFormat": "{{ project }} - {{ health_status }}"
            }
         ],
         "title": "Application Health Status",
         "type": "timeseries"
      },
      {
         "datasource": {
            "type": "prometheus",
