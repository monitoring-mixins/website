               "format": "table",
               "instant": true
            }
         ],
         "title": "Unhealthy Applications",
         "transformations": [
            {
               "id": "organize",
               "options": {
                  "excludeByName": {
                     "Time": true,
                     "Value": true,
                     "dest_server": true,
                     "job": true
                  },
                  "indexByName": {
                     "exported_namespace": 1,
                     "health_status": 3,
                     "name": 0,
                     "project": 2
                  },
                  "renameByName": {
                     "dest_server": "Cluster",
                     "exported_namespace": "Application Namespace",
                     "health_status": "Health Status",
                     "job": "Job",
                     "name": "Application",
                     "project": "Project"
                  }
