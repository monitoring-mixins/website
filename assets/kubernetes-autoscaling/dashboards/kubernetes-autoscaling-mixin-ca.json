{
   "__inputs": [ ],
   "__requires": [ ],
   "annotations": {
      "list": [ ]
   },
   "description": "A dashboard that monitors the Cluster Autoscaler. The dashboards were generated using [kubernetes-autoscaling-mixin](https://github.com/adinhodovic/kubernetes-autoscaling-mixin). Open issues and create feature requests in the repository.",
   "editable": true,
   "links": [
      {
         "asDropdown": true,
         "includeVars": false,
         "keepTime": true,
         "tags": [
            "kubernetes",
            "autoscaling",
            "kubernetes-autoscaling-mixin"
         ],
         "targetBlank": true,
         "title": "Kubernetes / Autoscaling",
         "type": "dashboards"
      }
   ],
