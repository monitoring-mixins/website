      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The total number of nodes in the cluster.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            },
            "overrides": [ ]
         },
         "gridPos": {
            "h": 4,
            "w": 3,
            "x": 0,
            "y": 1
         },
         "id": 2,
         "options": {
            "graphMode": "area",
            "percentChangeColorMode": "standard",
            "showPercentChange": false
         },
         "pluginVersion": "v11.4.0",
         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "round(\n  sum(\n    cluster_autoscaler_nodes_count{\n      cluster=\"$cluster\",\njob=~\"$job\"\n\n    }\n  )\n)\n",
