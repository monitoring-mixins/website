         "targets": [
            {
               "datasource": {
                  "type": "prometheus",
                  "uid": "$datasource"
               },
               "expr": "round(\n  count(\n    (\n      avg by (node_group) (\n        cluster_autoscaler_node_group_target_count{\n          cluster=\"$cluster\",\njob=~\"$job\"\n\n        }\n      ) /\n      max by (node_group) (\n        cluster_autoscaler_node_group_max_count{\n          cluster=\"$cluster\",\njob=~\"$job\"\n\n        }\n      )\n    ) >= 0.99\n  ) /\n  count(\n    cluster_autoscaler_node_group_max_count{\n      cluster=\"$cluster\",\njob=~\"$job\"\n\n    }\n  ) * 100\n) or vector(0)\n"
            }
