      {
         "datasource": {
            "type": "prometheus",
            "uid": "$datasource"
         },
         "description": "The total number of nodes in the cluster.",
         "fieldConfig": {
            "defaults": {
               "mappings": [ ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "green",
                        "value": 0
                     }
                  ]
               },
               "unit": "short"
            },
            "overrides": [ ]
