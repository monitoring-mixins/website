         "fieldConfig": {
            "defaults": {
               "mappings": [
                  {
                     "options": {
                        "0": {
                           "color": "red",
                           "text": "No"
                        },
                        "1": {
                           "color": "green",
                           "text": "Yes"
                        }
                     },
                     "type": "value"
                  }
               ],
               "thresholds": {
                  "steps": [
                     {
                        "color": "red",
                        "value": 0
                     },
                     {
                        "color": "green",
                        "value": 0.10000000000000001
                     }
                  ]
               },
               "unit": "short"
            },
            "overrides": [ ]
